{
  "name": "edx-marketing-site-frontend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "NODE_ENV=production AWS_PROFILE=claudia CDN_ROOT=https://d17shfn88f2e0o.cloudfront.net S3_BUCKET_NAME=clintonb-edx-marketing webpack -p",
    "compile-translations": "compile-json ./locale ./public/i18n",
    "deploy": "npm run compile-translations && npm run build && AWS_PROFILE=claudia claudia update && AWS_PROFILE=claudia node scripts/invalidate-cloudfront.js",
    "extract-translations": "jsxgettext --join-existing -L jinja --output-dir=./locale/templates/LC_MESSAGES --output=messages.pot `find . -name '*.html' | grep -v node_modules | grep -v \".git\"` && merge-po ./locale",
    "pull-translations": "tx pull",
    "push-translations": "tx push -s",
    "update-translations": "npm run extract-translations && npm run pull-translations && npm run compile-translations",
    "start": "nodemon --watch . --watch .env ./bin/www",
    "watch": "webpack --watch --display-error-details"
  },
  "files": [
    "bin",
    "config",
    "locale",
    "public",
    "routes",
    "services",
    "views",
    ".env",
    "app.js",
    "lambda.js"
  ],
  "dependencies": {
    "@edx/edx-bootstrap": "^2.2.2",
    "aws-serverless-express": "^3.3.6",
    "dotenv": "^8.2.0",
    "express": "~4.17.3",
    "i18n-abide": "0.0.26",
    "memory-cache": "^0.2.0",
    "moment": "^2.29.2",
    "morgan": "~1.9.1",
    "nunjucks": "^3.2.0",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7"
  },
  "devDependencies": {
    "ajv": "^5.5.2",
    "bootstrap": "^4.3.1",
    "bootstrap.native": "^2.0.27",
    "claudia": "^5.9.0",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^3.2.0",
    "debug": "^2.6.9",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "jsxgettext": "^0.11.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.13.1",
    "nodemon": "^1.19.4",
    "preload-webpack-plugin": "^3.0.0-beta.4",
    "purify-css": "^1.2.5",
    "purifycss-webpack": "^0.7.0",
    "sass-loader": "^6.0.7",
    "style-loader": "^1.0.0",
    "url-loader": "^2.2.0",
    "webpack": "^4.41.2",
    "webpack-bundle-tracker": "^0.4.3",
    "webpack-cli": "^3.3.9",
    "webpack-s3-plugin": "^1.0.3"
  }
}
